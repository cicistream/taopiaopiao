/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
@font-face {
  font-family: 'iconfont';  /* project id 374549 */
  src: url('http://at.alicdn.com/t/font_cqpmf31dz3g6tj4i.eot');
  src: url('http://at.alicdn.com/t/font_cqpmf31dz3g6tj4i.eot?#iefix') format('embedded-opentype'),
  url('http://at.alicdn.com/t/font_cqpmf31dz3g6tj4i.woff') format('woff'),
  url('http://at.alicdn.com/t/font_cqpmf31dz3g6tj4i.ttf') format('truetype'),
  url('http://at.alicdn.com/t/font_cqpmf31dz3g6tj4i.svg#iconfont') format('svg');
}
.iconfont{
    font-family:"iconfont" !important;
    font-size:16px;font-style:normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
    -moz-osx-font-smoothing: grayscale;
}